					<outputDirectory>${basedir}/target</outputDirectory>
					-->
					<suiteXmlFiles>
						<!-- TestNG suite XML files -->
						<suiteXmlFile>testng1.xml</suiteXmlFile>
					</suiteXmlFiles>
				</configuration>
			</plugin>
			 
  	</plugins> 
  </build>
  </profile>
  
